                "logs:CreateLogGroup",
                "logs:DescribeLogStreams",
                "logs:PutLogEvents",
                "logs:PutRetentionPolicy"
            ],
            "Resource": [
                "arn:aws:logs:us-east-1:938398057556:log-group:/aws/eks/protein-engineering-cluster-new/*:*",
